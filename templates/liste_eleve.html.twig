{% extends 'base.html.twig' %}

{% block title %}Liste des eleves{% endblock %}

{% block body %}

<h1 class="text-center mb-4">Liste des Eleves</h1>

    {% for student in eleve %}

  <div class="card mb-3 col-4 mx-2 p-2 text-center">
			<div class="no-gutters container">
				<div class="col-md-8">
					<div class="card-body">
						<h5 class="card-title text-center">{{student.nom}}</h5>
						<p class="card-text text-center">{{student.prenom}}</p>
						
					</div>
				</div>
			</div>
		</div>
      {% endfor %}

{% endblock %}